{
	"name": "basic_demo_gl3",
	"description": "Ported version of Dear ImGui C++ example using GLFW & Derelict OpenGL3",
	"targetType": "executable",
	"targetPath": "bin",
	"workingDirectory": "bin",
	"dependencies": {
		"bindbc-glfw": "~>0.10.0",
		"bindbc-opengl": "~>0.12.0",
		"imgui-d": {
			"path": "../../",
			"version": ">=0.0.0"
		}
	},
	"versions": ["GLFW_33", "GL_46"],
	"libs": [
		"imgui",
	],
	"lflags-windows": [
		"/LIBPATH:bin",
		"/NODEFAULTLIB:libcmt",
		"/NODEFAULTLIB:libvcruntime",
		"/NODEFAULTLIB:libucrt"
	],
	"configurations": [
		{
			"lflags-windows": [
				"/LIBPATH:bin-x86"
			],
			"name": "x86",
			"platforms": ["x86"],
			"targetPath": "bin-x86",
			"targetType": "executable",
			"workingDirectory": "bin-x86"
		},
		{
			"name": "x64",
			"platforms": ["x86_64"]
		}
	]
}